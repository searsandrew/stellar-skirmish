{
    "name": "jamrfun/stellar-skirmish",
    "description": "Core game engine for Stellar Empire: Skirmish.",
    "type": "library",
    "license": "MIT",
    "autoload": {
	"psr-4": {
	    "StellarSkirmish\\": "src/"
	}
    },
    "autoload-dev": {
	"psr-4": {
	    "StellarSkirmish\\Tests\\": "tests/"
	}
    },
    "authors": [
        {
            "name": "Andrew Sears",
            "email": "andrew@mayfifteenth.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "symfony/string": "^7.2"
    },
    "require-dev": {
	"pestphp/pest": "^3.0"
    },
    "config": {
        "platform": {
            "php": "8.3.6"
        },
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
